{
  "name": "ts-library",
  "version": "1.0.0",
  "author": "hanjeahwan@gmail.com",
  "description": "typescript library boilerplate",
  "private": true,
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
